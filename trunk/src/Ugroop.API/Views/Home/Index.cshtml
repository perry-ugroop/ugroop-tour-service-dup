    function ParseX() {
        $.ajax({
            url: "/Test/TestAAA",
            type: 'get',
            success: function (data) {
                var items = JSON.parse(data);
              
                $.each(items, function (item, value) {
                    $("#divTest").append(item + ' : ' + value + ' / ');
                });
            },
            error: function (e) {
                console.log(e.message);
            }
        });
    }

    function ParseBBB() {
        $.ajax({
            url: "/Test/TestBBB",
            type: 'get',
            success: function (data) {
                var items = JSON.parse(data);
                for (var i = 0; i < items.length; i++) {
                    $.each(items[i], function (field, value) {
                        $("#divTest").append(field + ' : ' + value + ' / ');
                    });
                }
            },
            error: function (e) {
                console.log(e.message);
            }
        });
    }

